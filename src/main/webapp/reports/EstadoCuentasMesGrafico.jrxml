	<queryString>
		<![CDATA[SELECT 
CUENTA.NOMCTA, CUENTA.XNICTA, CUENTA.CUENTA, NVL(CUENTA.SCTA,'') SCTA, NVL(CUENTA.SSCTA,'') SSCTA,
NVL(CUENTA.SSSCTA,'') SSSCTA, NVL(CUENTA.SSSSCTA,'')SSSSCTA, 0 SALINI, CUENTA.STACTA
, 1 MESPOL
, 1 CONPOL
, $P!{P_CAMPO3} ANOPOL
, 1 RENPOL
,  SUM(case when CUENTA.STACTA='D' THEN CUENTA.SALINI  ELSE 0 END) CANPOL
,  SUM(case when CUENTA.STACTA='A' THEN CUENTA.SALINI ELSE 0 END) CANPOLH
, 'SALDO INICIAL '||$P!{P_CAMPO3} CONCEP
,'' DN
, 1 REFPOL
,'01/01/'||$P!{P_CAMPO3} FECPOL
,'E' TIPPOL
FROM CUENTA
left  outer JOIN POLIDE
ON  (CUENTA.CUENTA  = POLIDE.CUENTA) 
AND (CUENTA.SCTA    = NVL(POLIDE.SCTA,'')) 
AND (CUENTA.SSCTA   = NVL(POLIDE.SSCTA,'')) 
AND (CUENTA.SSSCTA  = NVL(POLIDE.SSSCTA,'')) 
AND (CUENTA.SSSSCTA = NVL(POLIDE.SSSSCTA,''))  
AND CUENTA.IDSECTOR = POLIDE.IDSECTOR
WHERE 1 = 1 
AND CUENTA.CUENTA = $P{P_CUENTA}
AND CUENTA.NIVCTA = 'S' 
AND CUENTA.IDSECTOR =  $P{P_SECTOR} 
AND (POLIDE.MESPOL <= $P{P_MES} OR POLIDE.MESPOL IS NULL)
AND (($P{P_SCTA} IS NULL OR $P{P_SCTA} = '' OR CUENTA.SCTA = $P{P_SCTA} ) 
AND ($P{P_SSCTA} IS NULL OR $P{P_SSCTA} = '' OR CUENTA.SSCTA = $P{P_SSCTA}  ) 
AND ($P{P_SSSCTA} IS NULL OR $P{P_SSSCTA} = '' OR CUENTA.SSSCTA = $P{P_SSSCTA}  ) 
AND ($P{P_SSSSCTA} IS NULL OR $P{P_SSSSCTA} = '' OR CUENTA.SSSSCTA = $P{P_SSSSCTA}))
AND POLIDE.RENPOL IS NOT NULL
GROUP BY CUENTA.NOMCTA, CUENTA.XNICTA, CUENTA.CUENTA, CUENTA.SCTA, CUENTA.SSCTA,CUENTA.SSSCTA, CUENTA.SSSSCTA , CUENTA.STACTA

UNION

SELECT 
CUENTA.NOMCTA, CUENTA.XNICTA, CUENTA.CUENTA, CUENTA.SCTA, CUENTA.SSCTA,
CUENTA.SSSCTA, CUENTA.SSSSCTA, CUENTA.SALINI, CUENTA.STACTA,
POLIDE.MESPOL, POLIDE.CONPOL, POLIDE.ANOPOL, POLIDE.RENPOL,
NVL(POLIDE.CANPOL, 0) CANPOL, NVL(POLIDE.CANPOLH, 0) CANPOLH, POLIDE.CONCEP, POLIDE.DN,
POLIDE.REFPOL, POLIEN.FECPOL, POLIDE.TIPPOL
FROM CUENTA
LEFT OUTER JOIN POLIDE ON
(
	(CUENTA.CUENTA  = POLIDE.CUENTA) 
AND (CUENTA.SCTA    = NVL(POLIDE.SCTA,'')) 
AND (CUENTA.SSCTA   = NVL(POLIDE.SSCTA,'')) 
AND (CUENTA.SSSCTA  = NVL(POLIDE.SSSCTA,'')) 
AND (CUENTA.SSSSCTA = NVL(POLIDE.SSSSCTA,''))  
AND CUENTA.IDSECTOR = POLIDE.IDSECTOR
) 
LEFT OUTER JOIN POLIEN ON ((POLIDE.TIPPOL=POLIEN.TIPPOL) 
AND (POLIDE.MESPOL=POLIEN.MESPOL) AND (POLIDE.CONPOL= POLIEN.CONPOL)
AND (POLIDE.ANOPOL=POLIEN.ANOPOL) AND POLIDE.IDSECTOR = POLIEN.IDSECTOR)

WHERE 1 = 1 
AND CUENTA.CUENTA = $P{P_CUENTA}
AND CUENTA.NIVCTA = 'S' 
AND CUENTA.IDSECTOR =  $P{P_SECTOR} 
AND (POLIDE.MESPOL <= $P{P_MES} OR POLIDE.MESPOL IS NULL)
AND (($P{P_SCTA} IS NULL OR $P{P_SCTA} = '' OR CUENTA.SCTA = $P{P_SCTA} ) 
AND ($P{P_SSCTA} IS NULL OR $P{P_SSCTA} = '' OR CUENTA.SSCTA = $P{P_SSCTA}  ) 
AND ($P{P_SSSCTA} IS NULL OR $P{P_SSSCTA} = '' OR CUENTA.SSSCTA = $P{P_SSSCTA}  ) 
AND ($P{P_SSSSCTA} IS NULL OR $P{P_SSSSCTA} = '' OR CUENTA.SSSSCTA = $P{P_SSSSCTA}))
AND POLIDE.RENPOL IS NOT NULL


ORDER BY  CUENTA, SCTA, SSCTA, SSSCTA, SSSSCTA, $P!{P_ORDER_BY} , DN]]>
	</queryString>
