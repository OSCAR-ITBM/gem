	<queryString language="SQL">
		<![CDATA[SELECT * FROM (SELECT  TV.ID_ROW,
        MAX(DECODE(TE.NOMBRE, 'SEMESTRE', TV.VALOR, '')) SEMESTRE,
        MAX(DECODE(TE.NOMBRE, 'FECHAING', TV.VALOR, '')) FECHAING,
        MAX(DECODE(TE.NOMBRE, 'IDSECTOR', TV.VALOR, '')) ID_SECTOR,
        MAX(DECODE(TE.NOMBRE, 'IDANIO', TV.VALOR, '')) ID_ANIO,
        MAX(DECODE(TE.NOMBRE, 'CERTIFICACION', TV.VALOR, '')) CERTIFICACION,
        MAX(DECODE(TE.NOMBRE, 'EXPERIENCIA', TV.VALOR, '')) EXPERIENCIA,
        MAX(DECODE(TE.NOMBRE, 'TITULO', TV.VALOR,'')) TITULO
    FROM TR_ETIQ_TABLAS AS ET
        INNER JOIN TC_ETIQUETAS AS TE ON ET.ID_ETIQUETA = TE.ID
        INNER JOIN TC_TABLAS AS TT ON ET.ID_TABLA = TT.ID
        LEFT JOIN TC_VALORES AS TV ON ET.ID = TV.ID_ETIQ_TABLA
WHERE 1=1
    AND ET.STATUS = 1
    AND TT.NOMBRE = 'MEDIADOR'
GROUP BY TV.ID_ROW) T1 WHERE SEMESTRE=$P{pSemestre}]]>
	</queryString>
