	<queryString>
		<![CDATA[SELECT C.TIPPOL,
       C.TIPNOM,
       COUNT (P.ID) CUANTAS,
      SUM(NVL(P.CDEBE,0)) DEBE,
       SUM(NVL(P.CHABER,0))HABER 
    FROM CATTPO C
LEFT JOIN POLIEN P ON P.TIPPOL=C.TIPPOL AND P.IDSECTOR=$P{p_IdSector} AND P.MESPOL=$P{p_Mes}
  GROUP BY C.TIPPOL,
         C.TIPNOM]]>
	</queryString>
